{
    "name": "indigophp/fuel-stash",
    "type": "fuel-package",
    "description": "Replacement for Fuel Cache class",
    "license": "MIT",
    "keywords": ["fuel", "cache"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "indigophp/fuel-dependency": "dev-develop",
        "tedivm/stash": "~0.12.0"
    },
    "require-dev": {
        "codeception/codeception": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Fuel\\Stash\\": "classes/"
        }
    },
    "extra": {
        "installer-name": "stash"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
